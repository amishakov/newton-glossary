{
  "name": "clicktonext/kirby-clear-cache",
  "description": "This plugin allows you to clean temporary files and folders such as cache, media, .lock and .logins on your website.",
  "version": "1.0.0",
  "type": "kirby-plugin",
  "license": "MIT",
  "keywords": [
    "clicktonext",
    "kirby",
    "kirby-3",
    "kirby3-plugin",
    "kirby-plugin",
    "kirby-cache",
    "cache-clear"
  ],
  "homepage": "https://clicktonext.com",
  "support": {
    "email": "support@clicktonext.com"
  },
  "authors": [
    {
      "name": "Click to Next",
      "email": "contact@clicktonext.com",
      "homepage": "https://clicktonext.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "getkirby/composer-installer": "^1.2"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "installer-name": "kirby-cache-clear"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
